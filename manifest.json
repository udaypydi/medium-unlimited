{
    "name": "medium-unlimited",
    "version": "1.1",
    "description": "Access unlimited articles on medium",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "storage",
        "cookies",
        "https://medium.com/*",
        "tabs"
    ]
  }
